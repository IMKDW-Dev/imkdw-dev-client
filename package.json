{
  "name": "imkdw-dev-client",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "------------------------ PACKAGES ------------------------": "",
    "ui": "pnpm -F @imkdw-dev-client/ui",
    "utils": "pnpm -F @imkdw-dev-client/utils",
    "configs": "pnpm -r -F @imkdw-dev-client/config",
    "------------------------ APPS ------------------------": "",
    "blog-web": "pnpm -F @imkdw-dev-client/blog-web",
    "brain-web": "pnpm -F @imkdw-dev-client/brain-web"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.0.9",
    "next": "^15.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.9"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.2.0",
    "@eslint/eslintrc": "^3",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint-config-prettier": "^10.0.2",
    "postcss": "^8",
    "prettier": "^3.5.2",
    "turbo": "^2.4.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=22"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
